<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="caldera_cycleways_frontpage" path="/">
        <default key="_controller">AppBundle:Frontpage:index</default>
    </route>

    <route id="caldera_cycleways_incident_list_city" path="/{citySlug}/list">
        <default key="_controller">AppBundle:Incident/Incident:list</default>
    </route>
</routes>
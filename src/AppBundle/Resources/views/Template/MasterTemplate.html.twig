<!DOCTYPE html>
<html lang="en">
<head>
    {% include('AppBundle:Template:Includes/_meta.html.twig') %}

    <title>{% block title %}{% endblock %} â€” Cycleways</title>

    <link rel="stylesheet" href="{{ asset('bundles/app/css/cycleways.css') }}"/>
    <link rel="stylesheet" href="{{ asset('bundles/app/css/external/bootstrap4/bootstrap.min.css') }}"/>
    <link rel="stylesheet" href="{{ asset('bundles/app/css/external/leaflet/leaflet-0.7.7.css') }}"/>
    <link rel="stylesheet" href="{{ asset('bundles/app/css/external/leaflet/extramarkers.css') }}"/>
    <link rel="stylesheet" href="{{ asset('bundles/app/css/external/fontawesome/font-awesome.css') }}"/>
    <link rel="stylesheet" href="{{ asset('bundles/app/css/external/dropzone/dropzone.min.css') }}"/>
    <link rel="stylesheet" href="{{ asset('bundles/app/css/external/bootstrap-datepicker/bootstrap-datepicker.min.css') }}"/>

    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
    <script src="{{ asset('bundles/app/js/external/requirejs/require.js') }}"></script>
    <script src="{{ asset('bundles/app/js/Cycleways.js') }}"></script>

    {% block additionalHeaderElements %}{% endblock %}
</head>

<body class="{% block bodyClass %}{% endblock %}">
{% if 0 and not app.user %}
    <div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
         aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                {{ render(controller('CalderaCriticalmassSiteBundle:Security:loginFormModal' )) }}
            </div>
        </div>
    </div>
{% endif %}

<a href="https://github.com/calderacc/cycleways">
    <img style="position: absolute; top: 40px; right: 0; border: 0;" src="https://camo.githubusercontent.com/e7bbb0521b397edbd5fe43e7f760759336b5e05f/68747470733a2f2f73332e616d617a6f6e6177732e636f6d2f6769746875622f726962626f6e732f666f726b6d655f72696768745f677265656e5f3030373230302e706e67" alt="Fork me on GitHub" data-canonical-src="https://s3.amazonaws.com/github/ribbons/forkme_right_green_007200.png" />
</a>

{% block masterContent %}{% endblock %}

<script src="{{ asset('bundles/app/js/external/jquery/jquery-3.1.0.min.js') }}"></script>
<script src="{{ asset('bundles/app/js/external/bootstrap4/bootstrap.min.js') }}"></script>

{% block additionalFooterElements %}{% endblock %}
</body>
</html>

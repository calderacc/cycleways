<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="caldera.cycleways.admin.incident" class="Caldera\Bundle\CyclewaysBundle\Admin\IncidentAdmin">
            <tag name="sonata.admin" manager_type="orm" group="Inhalt" label="Gefahrenstellen" />
            <argument />
            <argument>Caldera\Bundle\CyclewaysBundle\Entity\Incident</argument>
            <argument />
            <call method="setTranslationDomain">
                <argument>CalderaCyclewaysBundle</argument>
            </call>
        </service>

        <service id="caldera.cycleways.admin.user" class="Caldera\Bundle\CyclewaysBundle\Admin\UserAdmin">
            <tag name="sonata.admin" manager_type="orm" group="Nutzer" label="Benutzer" />
            <argument />
            <argument>Caldera\Bundle\CyclewaysBundle\Entity\User</argument>
            <argument />
            <call method="setTranslationDomain">
                <argument>CalderaCyclewaysBundle</argument>
            </call>
        </service>
    </services>
</container>

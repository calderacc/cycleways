{% extends 'CalderaCyclewaysBundle:Template:MasterTemplate.html.twig' %}

{% block masterContent %}
    {% include('CalderaCyclewaysBundle:Template:Includes/_navigation.html.twig') with { 'fullscreenMap': 'false' } %}

    {% block breadcrumbContainer %}
        <div class="container mt-2 mb-2">
            <ol class="breadcrumb">
                <li class="breadcrumb-item">
                    <a href="{{ path('caldera_cycleways_frontpage') }}">
                        Cycleways
                    </a>
                </li>
                {% block breadcrumb %}{% endblock %}
            </ol>
        </div>
    {% endblock %}

    {% block content %}{% endblock %}

    {% include('CalderaCyclewaysBundle:Template:Includes/_adserver.html.twig') %}

    {% include('CalderaCyclewaysBundle:Template:Includes/_footer.html.twig') %}

    {% include('CalderaCyclewaysBundle:Template:Includes/_piwik.html.twig') %}

    <script>
        CriticalMass.loadModule('Notification');
    </script>
{% endblock %}
